      <array key="content_html">
         <array>
            <string>Lette Bølge! Naar du blaaner,</string>
            <map>
               <number key="num">1</number>
            </map>
         </array>
         <array>
            <string>Gjennemsigtig, lys og klar,</string>
            <map>
               <number key="num">2</number>
            </map>
         </array>
         <array>
            <string>Himlens Farveskjær du laaner,</string>
            <map>
               <number key="num">3</number>
            </map>
         </array>
         <array>
            <string>Selv du ingen Farve har.</string>
            <map>
               <number key="num">4</number>
            </map>
         </array>
         <array>
            <string>Himlen ei, kun Himlens Billed</string>
            <map>
               <number key="num">5</number>
               <number key="displayNum">5</number>
            </map>
         </array>
         <array>
            <string>Hviler i din dybe Favn;</string>
            <map>
               <number key="num">6</number>
            </map>
         </array>
         <array>
            <string>Aldrig er din Længsel stillet,</string>
            <map>
               <number key="num">7</number>
            </map>
         </array>
         <array>
            <string>Den er evig, som dit Savn.</string>
            <map>
               <number key="num">8</number>
            </map>
         </array>
         <array>
            <string/>
         </array>
         <array>
            <string>Bølge! Hvor du klarest rinder,</string>
            <map>
               <number key="num">9</number>
            </map>
         </array>
         <array>
            <string>Speiles Himlen i dit Bad;</string>
            <map>
               <number key="num">10</number>
               <number key="displayNum">10</number>
            </map>
         </array>
         <array>
            <string>Ak! din Længsel Hjertet minder</string>
            <map>
               <number key="num">11</number>
            </map>
         </array>
         <array>
            <string>Om hvad Skjæbnen skiller ad.</string>
            <map>
               <number key="num">12</number>
            </map>
         </array>
         <array>
            <string>Hjerte! Du bør ikke klage!</string>
            <map>
               <number key="num">13</number>
            </map>
         </array>
         <array>
            <string>Selv Naturen føler Savn;</string>
            <map>
               <number key="num">14</number>
            </map>
